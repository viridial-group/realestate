{
  "name": "@viridial/shared",
  "version": "1.0.32",
  "type": "module",
  "description": "Shared modules for Real Estate frontend applications - Vue 3, TypeScript, Pinia, Axios",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "USER-MODULE.md",
    "package.json",
    "LICENSE"
  ],
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "pinia",
    "axios",
    "real-estate",
    "shared",
    "modules",
    "frontend",
    "microservices"
  ],
  "author": {
    "name": "Viridial Dev",
    "email": "support@viridial.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/realestate.git",
    "directory": "frontend/shared"
  },
  "bugs": {
    "url": "https://github.com/your-org/realestate/issues"
  },
  "homepage": "https://github.com/your-org/realestate#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "peerDependencies": {
    "axios": "^1.6.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.3.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "pinia": "^2.1.7",
    "vue": "^3.4.27",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "validate": "echo 'Validating package...' && npm run build && node -e \"console.log('Package is valid')\"",
    "publish:public": "npm publish --access public",
    "publish:beta": "npm publish --tag beta --access public",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
